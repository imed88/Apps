@model IEnumerable<Apps.Models.Tables.Admins>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
                <th>
                    @Html.DisplayNameFor(model => model.nameAdmin)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.emailAdmin)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.passwordAdmin)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.phoneAdmin)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.created)
                </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.nameAdmin)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.emailAdmin)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.passwordAdmin)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.phoneAdmin)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.created)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.idAdmin">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.idAdmin">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.idAdmin">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
